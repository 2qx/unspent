{
  "name": "@unspent/phi",
  "version": "0.0.1",
  "description": "a collection of anyone can spend contracts",
  "author": "2qx",
  "private":"true",
  "scripts": {
    "update:artifacts":"node script/updateArtifacts.js",
    "clean": "rm -fr dist/* && rm -fr jest/playwright/*",
    "prebuild": "yarn install",
    "build": "yarn clean && tsc ",
    "build:browser": "#npx webpack",
    "nuke": "rm -fr jest/playwright/* && rm -fr node_modules ",
    "test": "npx jest --runInBand  --logHeapUsage --forceExit"
  },
  "type": "module",
  "main": "dist/main/index.js",
  "types": "dist/main/index.d.ts",
  "keywords": [
    "bitcoin",
    "bitcoin cash",
    "bch",
    "tBCH",
    "cashscript",
    "testnet",
    "faucet"
