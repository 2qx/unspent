    "@types/jest": "^27.0.1",
    "@types/js-yaml": "^4.0.1",
    "@types/node": "^18.7.18",
    "jest": "^27.1.1",
    "jest-process-manager": "^0.3.1",
    "jest-runner": "^27.2.2",
    "mainnet-js": "0.5.4",
    "supertest": "^6.1.3",
